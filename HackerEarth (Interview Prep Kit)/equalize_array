package Practice;
import java.util.*;
public class equalizeArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]= {3,3,2,1,3};
		
		System.out.println(equalizeArray(arr));
	}
	
	static int equalizeArray(int[] arr) {
		int equal=0;
		
		// if all elements are diffrent.
		// if few are same few are diffrent/
		// if all are same.
		
		int nwarr[]=new int[101];
		
		for(int a:arr)
		{
			nwarr[a]+=1;
		}

		
		int max=Integer.MIN_VALUE;
		
		for(int a:nwarr)
		{
			if(a>max)
				max=a;
		}
		
		int diff=0;
		
		//if all distinct element
		if(max==1)
		{
			diff=arr.length-1;
		}
		
		else
		{
			for(int a:nwarr)
			{
				if(a!=max)
					diff+=a;
				
			}
		}
		
		
		return diff;
    }

}
